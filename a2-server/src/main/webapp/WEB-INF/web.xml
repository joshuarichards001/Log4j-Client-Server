<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>LogsServlet</servlet-name>
        <servlet-class>nz.ac.vuw.swen301.a2.server.LogsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogsServlet</servlet-name>
        <url-pattern>/logs</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StatsCSVServlet</servlet-name>
        <servlet-class>nz.ac.vuw.swen301.a2.server.StatsCSVServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StatsCSVServlet</servlet-name>
        <url-pattern>/statscsv</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StatsServlet</servlet-name>
        <servlet-class>nz.ac.vuw.swen301.a2.server.StatsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StatsServlet</servlet-name>
        <url-pattern>/stats</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StatsPNGServlet</servlet-name>
        <servlet-class>nz.ac.vuw.swen301.a2.server.StatsPNGServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StatsPNGServlet</servlet-name>
        <url-pattern>/statspng</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StatsXLSServlet</servlet-name>
        <servlet-class>nz.ac.vuw.swen301.a2.server.StatsXLSServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StatsXLSServlet</servlet-name>
        <url-pattern>/statsxls</url-pattern>
    </servlet-mapping>
</web-app>